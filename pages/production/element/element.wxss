/* pages/production/element/element.wxss */

page{
  display: flex;
  justify-content: center;
  background: #e0e0e0;
}

.button {
  width: 100%;
  height: 100rpx;
}

/* 去掉按钮边框 */
button::after {
  border: 0;
  /* 去圆角 */
  border-radius: 0;
}

button {
  line-height:60rpx;
  /* align-items: center; */
  /* justify-content: center; */
  background-color: #fc0;
  color: #fff;
  /* display: flex; */
  /* 去圆角 */
  border-radius: 0;
}

.return {
  float: left;
  height: 100rpx;
  width: 100rpx;
  /* 图片暖色调 */
  /* filter:sepia(50%) */
}

.submit {
  float: right;
  height: 100rpx;
  width: 100rpx;
  /* 图片暖色调 */
  /* filter:sepia(50%) */
}

.center {
  display: flex;
  justify-content: center;
  align-items: center;
}

.bottom {
  width: 100%;
  position: fixed;
  bottom: 0rpx;
}

.border {
  border: 2px solid black;
}

  