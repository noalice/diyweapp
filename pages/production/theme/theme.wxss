.container{
  width: 100%;
  height: 100%;
  background-color: #550506
}

.swiper-block{

height: 1080rpx;
width: 100%;
  background-color: #550506
}
.swiper-item{
display: flex;
flex-direction: column;
justify-content: center;
align-items: flex-start;
overflow:unset;
}
.slide-image{
height:200rpx;
width:200rpx;
border-radius: 50%;
box-shadow: 0px 0px 30rpx rgba(0, 0,0,.2);
margin: 0 auto;
}
.active{
transform: scale(1.8);
transition:all .2s ease-in 0s;
z-index: 20;
}
