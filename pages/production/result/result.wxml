<!--pages/production/result.wxml-->
<view style="height:60rpx">
  <navigator url='../element/element' open-type="redirect">
    <button  class='bt' style="background-image:url('{{returnimg}}');background-size:100% 100%;" hover-class='hover'></button>
  </navigator>
</view>

<!-- bindlongtap 长按事件-->
<view class="center">
  <canvas wx:if="{{production == 'C'}}" canvas-id="canvasC" class="canvas" style='width:100%;height:{{centerh}}rpx;position:relative;' bindlongtap='longtap'></canvas>
  <canvas wx:else canvas-id="canvasB" class="canvas" style='width:100%;height:{{centerh}}rpx;position:relative;' bindlongtap='longtap'></canvas>
</view>

<view style="height:80rpx;width:540rpx;display:flex;align-items:center;float:right;">
  <image class='img' mode="aspectFit" src="{{tipimg}}" style='height:50rpx;' />
</view>
<view style='height:80rpx;width:750rpx;display:flex;'>
  <image src='https://www.vrwbg.com:8080/mini/image_resources/USKP0501.png' mode='aspectFit' style='height:70%;width:70%;margin:auto'></image>
</view>